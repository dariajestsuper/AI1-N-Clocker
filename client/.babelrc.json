{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "jsxVanilla"
      }
    ],
    ["babel-plugin-jsx-pragmatic", {
      "module": "jsx-vanilla",
      "import": "jsxVanilla"
    }]
  ]
}
